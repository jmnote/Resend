{
	"name": "Resend",
	"version": "0.1.0",
	"author": [
		"jmnote"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Resend",
	"description": "Sends MediaWiki user email through Resend.",
	"license-name": "Apache-2.0",
	"requires": {
		"MediaWiki": ">= 1.43.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\Resend\\": "includes/"
	},
	"HookHandlers": {
		"ResendHooks": {
			"class": "MediaWiki\\Extension\\Resend\\Hooks"
		}
	},
	"Hooks": {
		"AlternateUserMailer": "ResendHooks"
	},
	"load_composer_autoloader": true,
	"config": {
		"ResendAPIKey": {
			"value": ""
		}
	},
	"manifest_version": 2
}
